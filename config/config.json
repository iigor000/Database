{
  "block": {
    "block_size": 4096
  },
  "wal": {
    "wal_segment_size": 10,
    "directory": "data/wal"
  },
  "memtable": {
    "num": 5,
    "num_entries": 10,
    "struct": "skiplist"
  },
  "skiplist": {
    "max_height": 16
  },
  "btree": {
    "min_size": 16
  },
  "sstable": {
    "use_compression": true,
    "summary_level": 10,
    "directory": "data/sstable",
    "single_file": false
  },
  "cache": {
    "capacity": 100
  },
  "lsmtree": {
    "max_level": 5,
    "compaction_algorithm": "size_tiered",
    "use_size_based_compaction": true,
    "max_level_size_mb": 100,
    "base_sstable_limit": 4,
    "level_size_multiplier": 10,
    "max_sstables_per_level": [4, 8, 16, 32, 64]
  }
}
