{
  "block": {
    "block_size": 4096
  },
  "wal": {
    "wal_segment_size": 10,
    "directory": "data/wal"
  },
  "memtable": {
    "num": 5,
    "num_entries": 10,
    "struct": "skiplist"
  },
  "skiplist": {
    "max_height": 16
  },
  "btree": {
    "min_size": 16
  },
  "sstable": {
    "use_compression": false,
    "summary_level": 10,
    "directory": "data/sstable",
    "single_file":  false
  },
  "cache": {
    "capacity": 100
  },
  "lsmtree": {
    "max_level": 5,
    "compaction_algorithm": "size_tiered",
    "base_sstable_limit": 10000,
    "level_size_multiplier": 10,
    "max_sstables_per_level": 8
  },
  "token_bucket": {
    "start_tokens": 1000,
    "refill_interval_s": 60
  },
  "compression": {
    "dictionary_dir": "data/compression_dict"
  }
}
